{
	"useTabs": true,
	"singleQuote": true,
	"trailingComma": "all",
	"printWidth": 80,
	"tabWidth": 4,
	"htmlWhitespaceSensitivity": "ignore",
	"plugins": [
		"prettier-plugin-tailwindcss",
		"prettier-plugin-tailwindcss-extra"
	],
	"overrides": [
		{
			"files": "*.templ",
			"options": { "parser": "tailwindcss-extra" }
		}
	]
}
